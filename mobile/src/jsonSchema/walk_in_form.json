{
  "$id": "#properties/WalkIn",
  "type": "object",
  "required": [
    "name",
    "gender",
    "nationalId",
    "dob",
    "email",
    "phone",
    "addressLine1",
    "district",
    "state",
    "pincode",
    "identityType",
    "identityValue"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "Name"
    },
    "gender": {
      "title": "Select Gender",
      "type": "string",
      "enum": [
        "Male",
        "Female",
        "Other"
      ]
    },
    "nationalId": {
      "title": "Nationality",
      "type": "string",
      "$id": "#/properties/nationalId",
      "enum": [
        "Indian",
        "Other"
      ],
      "default": "Indian"
    },
    "dob": {
      "title": "Date of Birth",
      "type": "string",
      "$id": "#/properties/dob",
      "format": "date"
    },
    "email": {
      "title": "Email",
      "type": "string",
      "$id": "#/properties/email",
      "format": "email"
    },
    "phone": {
      "title": "Mobile No.",
      "type": "string",
      "$id": "#/properties/phone",
      "format": "phone-in"
    },
    "identityType": {
      "title": "Identity Type",
      "$id": "#/properties/identityType",
      "enum": [
        "did:in.gov.uidai.aadhaar",
        "did:PAN"
      ],
      "enumNames": [
        "Aadhaar",
        "PAN"
      ]
    },
    "identityValue": {
      "title": "Identity Number",
      "$id": "#/properties/identityValue",
      "type": "string"
    },
    "state": {
      "$id": "#/properties/state",
      "type": "string",
      "title": "State"
    },
    "district": {
      "$id": "#/properties/district",
      "type": "string",
      "title": "District"
    },
    "addressLine1": {
      "$id": "#/properties/addressLine1",
      "type": "string",
      "title": "Locality"
    },
    "pincode": {
      "$id": "#/properties/pincode",
      "type": "number",
      "title": "Pincode"
    }
  }
}
